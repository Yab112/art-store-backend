{
  "name": "finder-api",
  "version": "0.0.1",
  "description": "Finder API ",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "pnpm": {
    "overrides": {
      "express": "4.21.2",
      "form-data": "^4.0.4",
      "body-parser": "^1.20.3",
      "path-to-regexp@^3": "^3.3.0",
      "path-to-regexp@<1": "^0.1.12",
      "cross-spawn": "^7.0.5",
      "@babel/core": "^7.26.10",
      "@babel/helpers": "^7.26.10",
      "@nestjs/common": "^10.4.16",
      "@nestjs/core": "^10.4.16",
      "@nestjs/platform-express": "^10.4.16"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build && npm run copy:templates",
    "copy:templates": "copyfiles src/libraries/email/templates/**/* dist/libraries/email/templates",
    "format": "prettier --write .",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e env/local.env -- nest start",
    "start:dev": "dotenv -e env/local.env -- nest start --watch",
    "start:debug": "dotenv -e env/local.env -- nest start --debug --watch",
    "start:prod": "dotenv -e env/production.env -- node dist/main",
    "start:staging": "dotenv -e env/staging.env -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:strict": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "type-check": "tsc --noEmit",
    "migrate:dev": "dotenv -e env/local.env -- prisma migrate dev --preview-feature",
    "migrate:dev:create": "dotenv -e env/local.env -- prisma migrate dev --create-only --preview-feature",
    "migrate:dev:pull": "dotenv -e env/local.env -- prisma db pull",
    "migrate:reset": "dotenv -e env/local.env -- prisma migrate reset --preview-feature",
    "migrate:deploy:prod": "dotenv -e env/production.env -- npx prisma migrate deploy --preview-feature",
    "migrate:deploy:staging": "dotenv -e env/test.env -- npx prisma migrate deploy --preview-feature",
    "migrate:deploy:dev": "dotenv -e env/local.env -- npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "db:seed": "dotenv -e env/local.env -- ts-node prisma/seed.ts",
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "postinstall": "npm run prisma:generate",
    "swagger:generate": "ts-node scripts/generate-swagger.ts",
    "swagger:export": "ts-node scripts/generate-swagger.ts --export",
    "swagger:validate": "ts-node scripts/generate-swagger.ts --validate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.4.1",
    "@prisma/client": "^6.18.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.15",
    "argon2": "^0.41.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "better-auth": "1.3.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "google-auth-library": "^9.14.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.5",
    "speakeasy": "^2.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.0.0",
    "prisma": "^6.16.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.2.0"
}
